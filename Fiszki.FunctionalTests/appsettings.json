{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "FiszkiDatabase": "Host=localhost;Port=5434;Database=fiszki_dev;Username=postgres;Password=postgres"
  },
  "Playwright": {
    "BaseUrl": "http://localhost:5290",
    "Headless": true,
    "SlowMotion": 0,
    "DefaultTimeout": 7000,
    "ViewportWidth": 1280,
    "ViewportHeight": 800
  },
  "OpenRouter": {
    "ApiKey": "",
    "BaseUri": "https://openrouter.ai/api/v1/",
    "Referer": "https://localhost:5001",
    "Title": "Fiszki - Polish Flashcard App",
    "RequestTimeout": "00:01:00",
    "MaxConcurrentRequests": 5,
    "EnableStreaming": false,
    "EnableStructuredValidation": true,
    "ModelCacheTtl": "00:30:00"
  }
}